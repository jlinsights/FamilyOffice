// 프로그램 페이지 타입 명확화
import { LucideIcon } from "lucide-react";

export interface MembershipStat {
  icon: LucideIcon;
  value: string;
  label: string;
  description: string;
}

export interface MemberBenefit {
  icon: LucideIcon;
  title: string;
  description: string;
  details: string[];
}

export interface ExclusiveProgram {
  name: string;
  frequency: string;
  description: string;
}

export interface ExclusiveProgramCategory {
  category: string;
  icon: LucideIcon;
  programs: ExclusiveProgram[];
}

export interface EducationPrograms {
  monthly: string[];
  quarterly: string[];
  annual: string[];
}

export interface SpecialProgram {
  title: string;
  subtitle?: string;
  desc: string;
  place: string;
  target: string;
  freq: string;
}
